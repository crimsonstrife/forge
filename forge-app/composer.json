{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "cocur/slugify": "^4.5",
        "czproject/git-php": "^4.2",
        "doctrine/dbal": "^4.0",
        "enshrined/svg-sanitize": "^0.20.0",
        "filament/actions": "^3.2",
        "filament/filament": "^3.2",
        "filament/forms": "^3.2",
        "filament/infolists": "^3.2",
        "filament/notifications": "^3.2",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "filament/spatie-laravel-settings-plugin": "^3.2",
        "filament/tables": "^3.2",
        "filament/widgets": "^3.2",
        "guzzlehttp/guzzle": "^7.9",
        "laravel/framework": "^11.9",
        "laravel/horizon": "^5.27",
        "laravel/jetstream": "^5.1",
        "laravel/passport": "^12.2",
        "laravel/pennant": "^1.10",
        "laravel/prompts": "^0.3.1",
        "laravel/reverb": "^1.0",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.11",
        "laravel/socialite": "^5.16",
        "laravel/tinker": "^2.9",
        "league/commonmark": "^2.5",
        "livewire/livewire": "^3.0",
        "maatwebsite/excel": "^3.1.56",
        "nesbot/carbon": "^3.7",
        "phpoffice/phpspreadsheet": "^2.3.0",
        "protonemedia/laravel-verify-new-email": "^1.10",
        "psr/simple-cache": "^3.0",
        "ramsey/uuid": "^4.7",
        "spatie/laravel-ignition": "^2.8",
        "spatie/laravel-medialibrary": "^11.8",
        "spatie/laravel-permission": "^6.9",
        "spatie/laravel-settings": "^3.3",
        "symfony/cache": "^7.1",
        "symfony/error-handler": "^7.1",
        "symfony/event-dispatcher": "^7.1",
        "symfony/mailer": "^7.1",
        "symfony/mime": "^7.1",
        "symfony/polyfill-php83": "^1.30",
        "symfony/uid": "^7.1",
        "webcreate/vcs": "^1.1",
        "xetaio/xetaravel-mentions": "^11.0"
    },
    "provide": {
        "phpoffice/phpspreadsheet": "1.29"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13",
        "barryvdh/laravel-ide-helper": "^3.1",
        "fakerphp/faker": "^1.23",
        "friendsofphp/php-cs-fixer": "^3.62.0",
        "laravel/pint": "^1.17",
        "laravel/pulse": "^1.2",
        "laravel/sail": "^1.31",
        "laravel/telescope": "^5.1",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^9.4",
        "phpunit/phpunit": "^11.0.1",
        "itsgoingd/clockwork": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
